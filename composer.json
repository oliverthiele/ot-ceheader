{
	"name": "oliverthiele/ot-ceheader",
	"type": "typo3-cms-extension",
	"description": "Adds an optional RTE-based header field (header_rte) with preheader and icon support for tt_content headers.",
	"keywords": ["typo3", "extension", "content-element", "header", "rte", "ckeditor"],
	"homepage": "https://www.oliver-thiele.de",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Oliver Thiele",
			"email": "mail@oliver-thiele.de",
			"homepage": "https://www.oliver-thiele.de",
			"role": "Developer"
		}
	],
	"require": {
		"ext-dom": "*",
		"ext-libxml": "*",
		"typo3/cms-core": "^13.4",
		"oliverthiele/ot-icons": "^1.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"conflict": {
	},
	"autoload": {
		"psr-4": {
			"OliverThiele\\OtCeheader\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ot_ceheader"
		}
	}
}
